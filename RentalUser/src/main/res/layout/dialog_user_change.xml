<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/carddialog_coner"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_map_dialog_baidu"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="百度导航"
            android:textColor="@drawable/dialog_text_bg"
            android:textSize="15dp" />

        <include layout="@layout/common_cut_match_rule" />

        <TextView
            android:id="@+id/tv_map_dialog_gaode"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="高德导航"
            android:textColor="@drawable/dialog_text_bg"
            android:textSize="15dp" />

    </LinearLayout>




    <TextView
        android:id="@+id/tv_map_dialog_cancle"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:text="取消"
        android:background="@drawable/carddialog_coner"
        android:textColor="@color/primary_highlight"
        android:textSize="15dp" />


</LinearLayout>